<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="0dp"
    android:layout_marginBottom="0dp"
    android:background="#D2F0F0"
    tools:context=".MainActivity">

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="310dp"
        android:layout_height="310dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="262dp">

            <Button
                android:id="@+id/b1"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />

            <View
                android:id="@+id/divider5"
                android:layout_width="5dp"
                android:layout_height="match_parent"
                android:background="#A41B5751" />

            <Button
                android:id="@+id/b2"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />

            <View
                android:id="@+id/divider4"
                android:layout_width="5dp"
                android:layout_height="match_parent"
                android:background="#A41B5751" />

            <Button
                android:id="@+id/b3"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />
        </TableRow>

        <TableRow
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:background="#A41B5751">

            <View
                android:id="@+id/divider11"
                android:layout_width="match_parent"
                android:layout_height="5dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/b4"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />

            <View
                android:id="@+id/divider9"
                android:layout_width="5dp"
                android:layout_height="match_parent"
                android:background="#A41B5751" />

            <Button
                android:id="@+id/b5"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />

            <View
                android:id="@+id/divider8"
                android:layout_width="5dp"
                android:layout_height="match_parent"
                android:background="#A41B5751" />

            <Button
                android:id="@+id/b6"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />
        </TableRow>

        <TableRow
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:background="#A41B5751">

            <View
                android:id="@+id/divider12"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="?android:attr/listDivider" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/b7"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />

            <View
                android:id="@+id/divider14"
                android:layout_width="5dp"
                android:layout_height="match_parent"
                android:background="#A41B5751" />

            <Button
                android:id="@+id/b8"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />

            <View
                android:id="@+id/divider13"
                android:layout_width="5dp"
                android:layout_height="match_parent"
                android:background="#A41B5751" />

            <Button
                android:id="@+id/b9"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:fontFamily="@font/bad_script"
                android:textColor="#00796B"
                android:textSize="25dp"
                app:backgroundTint="#D2F0F0" />
        </TableRow>


    </TableLayout>

    <Button
        android:id="@+id/button19"
        android:layout_width="150dp"
        android:layout_height="66dp"
        android:fontFamily="@font/bad_script"
        android:onClick="play"
        android:shadowColor="#AFB42B"
        android:text="Play"
        android:textColor="#00897B"
        android:textColorHighlight="#00897B"
        android:textSize="25dp"
        android:textStyle="bold"
        app:backgroundTint="#ABF8BA2D"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.905"
        app:strokeColor="#FBC02D" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/bad_script"
        android:textColor="#D88F57"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.241"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.244" />



</androidx.constraintlayout.widget.ConstraintLayout>